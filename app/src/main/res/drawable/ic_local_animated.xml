<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/local_filled"
        android:drawable="@drawable/ic_local_filled"
        android:state_checked="true" />

    <item
        android:id="@+id/local_outlined"
        android:drawable="@drawable/ic_local_outlined" />

    <transition
        android:drawable="@drawable/avd_local_from_filled"
        android:fromId="@+id/local_filled"
        android:toId="@+id/local_outlined" />

    <transition
        android:drawable="@drawable/avd_local_to_filled"
        android:fromId="@+id/local_outlined"
        android:toId="@+id/local_filled" />

</animated-selector>